{
  "expo": {
    "name": "Daily Tracker",
    "slug": "frontend_dt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_dailytracker.png",
    "scheme": "frontenddt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "neadyr.dailytracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_dailytracker.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#129954"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow dailyTracker to access your camera"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow dailyTracker to access your photos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22d8d8cc-2b2c-4e65-bfc6-ffbdf5689aaa"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/22d8d8cc-2b2c-4e65-bfc6-ffbdf5689aaa"
    }
  }
}
